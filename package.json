{
  "name": "yoti-node-starter",
  "version": "1.0.0",
  "description": "Node starter app for Yoti integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "author": "Rob Wilson, Corben Dredge",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.18.0",
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "mocha": "8.2.1",
    "nodemon": "2.0.7"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "helmet": "4.4.1",
    "method-override": "3.0.0",
    "winston": "2.4.5",
    "yoti": "3.14.0"
  }
}
