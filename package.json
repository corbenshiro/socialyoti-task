{
  "name": "yoti-node-starter",
  "version": "1.0.0",
  "description": "Node starter app for Yoti integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "prestart": "babel src --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "author": "Rob Wilson",
  "license": "MIT",
  "engine": "node 7.2.0",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-preset-es2015": "6.18.0",
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "mocha": "3.2.0",
    "nodemon": "1.11.0"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "cors": "2.8.1",
    "express": "4.14.0",
    "express-session": "1.14.2",
    "helmet": "3.1.0",
    "method-override": "2.3.7",
    "winston": "2.3.0",
    "yoti-node-sdk": "1.0.2"
  }
}
